<?xml version="1.0" encoding="utf-8"?>
<memcacheSetConfiguration>
  <cacheSets>
    <!--  缓存集定义  -->
    <!--  ExpireType 仅在模拟企业库过期机制时才有效  -->
    <cacheSet name="memcached30min" cacheManager="CacheManager1" TimeExpire="30"/>
    <cacheSet name="memcached10min" cacheManager="CacheManager1" TimeExpire="10"/>
    <cacheSet name="memcached5min" cacheManager="CacheManager1" TimeExpire="5"/>
    <cacheSet name="memcached1min" cacheManager="CacheManager1" TimeExpire="1"/>
  </cacheSets>
</memcacheSetConfiguration>
