@model HJD.CouponService.Contracts.Entity.CommOrderEntity
@using WHotelSite
@using WHotelSite.App_Code
@using WHotelSite.Params
@{
    Layout = null;
   
}
 
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>支付完成</title>
    <link href="/content/css/coupon/paycomplete.css" rel="stylesheet">
</head>
<body>

        <div class="main">
            <div class="tit" style="padding: 2.6em 0 3em 0;">
                <img src="~/Content/images/coupon/pay/yes-wire.png" alt="" />您已完成支付
            </div>
            <div class="hotel">
                <div class="left">支付内容</div>
                <div class="right">@Model.Name</div>
                <div style="clear:both;"></div>
            </div>
            <div class="hotel">
                <div class="left">支付金额</div>
                <div class="right">@Model.Price 元</div>
                <div style="clear:both;"></div>
            </div>
        </div> 
     

    <script src="~/Content/js/jquery-1.10.2-min.js"></script>
    <script src="~/Content/js/common.js"></script>
</body>
</html>
