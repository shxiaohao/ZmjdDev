@using WHotelSite
@using WHotelSite.App_Code
@using WHotelSite.Params
@{
    ViewBag.Title = string.Format("周末酒店");

    var cssVersion = "190001010000";
    try
    {
        var last = System.Reflection.Assembly.GetExecutingAssembly().Location;
        var lastTime = System.IO.File.GetLastWriteTime(last);
        cssVersion = lastTime.ToString("yyyyMMddhhmm");
    }
    catch (Exception ex) { }

    var udpate = ViewBag.Update;
}
@section styles
{
    <link href="/content/css/mail/mail.css?v=@cssVersion" rel="stylesheet">
}
<script>

</script>
<div style="width:100%;height:auto;min-height:300px;">
    <div style="width:100%;height:15em;line-height:15em;text-align:center;">
        @if (udpate > 0)
        {
            <span style="font-size:1.5em;">您已成功取消订阅</span>
        }
        else
        {
            <span style="font-size: 1.5em;">取消订阅失败</span>
        }
    </div>
</div>
<!--  -->
@section scripts
{
    @Scripts.Render("~/Content/js/mail/mail.js")
}